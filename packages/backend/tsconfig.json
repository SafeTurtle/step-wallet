{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "functions/build",
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "skipLibCheck": true,
    "resolveJsonModule": true,
  },
  "compileOnSave": true,
  "include": [
    "functions/src",
    "utlis",
    "functions/config.json"
  ]
}